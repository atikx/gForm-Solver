{
  "manifest_version": 3,
  "name": "gForm Solver",
  "version": "2.0.0",
  "description": "A web extension to automatically fill Google Forms with correct answers.",
  "permissions": ["activeTab", "scripting", "storage"],
  "background": {
    "service_worker": "background.js",
    "type": "module"
  },
  
  "host_permissions": ["<all_urls>"],
  "action": {
    "default_popup": "index.html",
    "default_icon": {
      "32" : "icons/icon32.jpg"
    }
  }
}
